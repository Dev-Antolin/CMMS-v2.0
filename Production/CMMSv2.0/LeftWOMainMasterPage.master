<%@ Master Language="VB" MasterPageFile="~/MainUserMasterPage.master" AutoEventWireup="false" CodeFile="LeftWOMainMasterPage.master.vb" Inherits="LeftWOMainMasterPage" %>

<asp:Content ID="Content1" ContentPlaceHolderID="CMMSLeftContentPlaceHolder" Runat="Server">
    <div id="leftpage">
        <div id="side-bar">
            <div>
                <p class="sideBarTitle">Work Order Menu</p>
                <ul>
                    <li>
                        <asp:LinkButton ID="CWO" href="../WorkOrder/CreateWO.aspx" runat="server">&rsaquo; Create Work Order</asp:LinkButton></li>
                    <li>
                        <asp:LinkButton ID="RWO" href="../WorkOrder/ReceivedWO.aspx" runat="server">&rsaquo; Received Work Order</asp:LinkButton></li>
                    <li>
                        <asp:LinkButton ID="OWO" href="../WorkOrder/OpenWO.aspx" runat="server">&rsaquo; Open Work Order</asp:LinkButton></li>
                    <li>
                        <asp:LinkButton ID="XWO" href="../WorkOrder/CloseWO.aspx" runat="server">&rsaquo; Closed Work Order</asp:LinkButton></li>
                    <li id="ViewBranch" runat ="server" visible ="false" >
                        <asp:LinkButton ID="View" href="../Update/MISView.aspx" runat="server">&rsaquo; View Branch Device</asp:LinkButton></li>
                </ul>
            </div>
            <div class="lighterBackground">
                <!-- ##### Display Link ##### -->
            </div>
            <div>
