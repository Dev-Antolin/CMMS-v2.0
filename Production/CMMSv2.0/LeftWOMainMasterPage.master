                    <td width="40px;">
                        <asp:Image ID="Image5" runat="server" ImageUrl="~/Images/job.gif" />
                    </td>
                    <td class="tdTxtBox">
                        <span id="Span2"></span>
                        <asp:Label ID="lblJob" runat="server" Text="Job description"></asp:Label>
                    </td>
                </tr>
                <tr>
                    <td width="40px;">
                        <asp:Image ID="Image4" runat="server" ImageUrl="~/Images/CostCenter1.PNG" />
                    </td>
                    <td class="tdTxtBox">
                        <span id="Span1"></span>
                        <asp:Label ID="lblCostCenter" runat="server" Text="Cost Center"></asp:Label>
                    </td>
                </tr>
                <tr>
                    <td width="40px;">
                        <asp:Image ID="Image1" runat="server" ImageUrl="~/Images/calendar.PNG" />
                    </td>
                    <td class="tdTxtBox">
                        <span id="jclock2"></span>
                    </td>
                </tr>
                <tr>
